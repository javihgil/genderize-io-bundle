{
    "name": "javihgil/genderize-io-bundle",
    "description": "Integrates GenderizerIoClient library into Symfony",
    "keywords": ["genderize.io","php","client","symfony"],
    "license": "MIT",
    "type": "library",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Jhg\\GenderizeIoBundle\\": ""
        }
    },
    "require-dev": {
        "phpunit/phpunit": "4.2.*",
        "mockery/mockery": "0.9.*"
    },
    "require": {
        "php": ">=5.4",
        "twig/twig": "~1.20",
        "javihgil/genderize-io-client": "self.version"
    },
    "scripts": {
        "test": [
            "vendor/bin/phpunit -c phpunit.xml"
        ],
        "coverage": [
            "vendor/bin/phpunit -c phpunit.xml --coverage-html target/reports/phpunit-coverage"
        ]
    }
}
